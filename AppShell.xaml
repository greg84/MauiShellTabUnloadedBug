<?xml version="1.0" encoding="UTF-8" ?>
<Shell
    x:Class="ShellTabsUnloadedBug.AppShell"
    xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
    xmlns:local="clr-namespace:ShellTabsUnloadedBug"
    Shell.FlyoutBehavior="Disabled">

    <TabBar Route="Main">
        <ShellContent Title="First Tab" ContentTemplate="{DataTemplate local:MainPage}" />
        <ShellContent Title="Second Tab" ContentTemplate="{DataTemplate local:AnotherPage}" />
    </TabBar>

</Shell>

